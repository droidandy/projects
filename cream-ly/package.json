  "scripts": {
    "start": "npm run build-blog && npm run build-translations && slate-tools start",
    "build": "npm run build-blog && npm run build-inventory && npm run build-products && npm run build-translations && npm run build-html-pages-for-seo && npm run build-seo-tags && npm run build-dist-copy && npm run build-webpack && npm run build-dist-changeJsEntry",
    "build-config": "node build-scripts/gitDataUpdate",
    "build-html-pages-for-seo": "ALLOW_SEO_GENERATOR=1 jest generateHtmlPages.test.ts",
    "build-seo-tags": "ts-node ./build-scripts/generateSEOMetaTags.ts",
    "build-translations": "node  --unhandled-rejections=strict build-scripts/translationsUpdate ",
    "build-inventory": "node  --unhandled-rejections=strict build-scripts/outOfStockUpdate",
    "build-products": "npx ts-node build-scripts/productsListUpdate",
    "build-blog": "npx ts-node build-scripts/blogArticlesUpdate",
    "build-storybook": "build-storybook -s ./src/assets,./src/scripts/react-components/EmailNotifications/templates",
    "build-webpack": "npx webpack-cli -c webpack.config.js --progress",
    "build-dist-copy": "npx ts-node build-scripts/distDirectoryCopy",
    "build-dist-changeJsEntry": "npx ts-node build-scripts/distChangeJsEntry",
    "webpack-analyzer": "kill-port 8888 && npx webpack-cli -c webpack.config.js --analyze ",
    "test": "jest",
    "lint": "slate-tools lint",
    "format": "slate-tools format",
    "storybook": "kill-port 6006 && npm run build-blog npm run build-inventory && npm run build-products && npm run build-translations && start-storybook -p 6006  -s ./src/assets,./src/scripts/react-components/EmailNotifications/templates",
    "storybook-deploy": "storybook-to-ghpages",
    "chromatic": "chromatic --project-token=n6rn0j3gchr"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-optional-chaining",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
