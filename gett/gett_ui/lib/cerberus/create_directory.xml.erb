   <soap:Header/>
   <soap:Body>
      <cer:CreateDirectoryRequest>
         <com:credentials>
            <com:user><%= soap_username %></com:user>
            <com:password><%= soap_password %></com:password>
         </com:credentials>
         <cer:directoryPath>D:/ftproot/<%= directory %></cer:directoryPath>
      </cer:CreateDirectoryRequest>
   </soap:Body>
</soap:Envelope>
