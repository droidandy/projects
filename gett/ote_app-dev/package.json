{
  "name": "oteApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "cold-start": "yarn start --reset-cache",
    "clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean && yarn",
    "eslint": "./node_modules/.bin/eslint src",
    "format": "./node_modules/.bin/eslint --fix .",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "android": "yarn cli run-android --variant=devDebug",
    "ios": "yarn cli run-ios --scheme=oteApp_dev",
    "test": "jest",
    "detox-build-ios": "./node_modules/.bin/detox build -c ios.sim.debug",
    "detox-build-android": "./node_modules/.bin/detox build -c android.emu.release",
    "test:e2e:ios": "tape -r ./e2e/setup ./e2e/runner --configuration ios.sim.debug",
    "test:e2e:android": "tape -r ./e2e/setup ./e2e/runner --configuration android.emu.release",
    "svg": "node --experimental-modules scripts/SVGConverter/index.mjs"
  },
  "dependencies": {
    "@svgr/core": "^4.1.0",
    "@svgr/plugin-jsx": "^4.1.0",
    "@svgr/plugin-prettier": "^4.0.3",
    "@svgr/plugin-svgo": "^4.0.3",
    "axios": "^0.18.0",
    "faye": "^1.2.4",
    "glob": "^7.1.3",
    "js-beautify": "^1.8.9",
    "lodash": "^4.17.11",
    "lottie-react-native": "^2.5.11",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-fast-compare": "^2.0.4",
    "react-native": "^0.57.8",
    "react-native-android-location-services-dialog-box": "^2.7.0",
    "react-native-animatable": "^1.3.0",
    "react-native-awesome-card-io": "^0.8.1",
    "react-native-cached-image": "github:fungilation/react-native-cached-image",
    "react-native-calendars": "^1.21.0",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "0.24.3",
    "react-native-extra-dimensions-android": "^1.0.0",
    "react-native-fabric": "^0.5.2",
    "react-native-firebase": "^5.1.1",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-gps": "^0.1.1",
    "react-native-haptic": "^1.0.1",
    "react-native-hyperlink": "^0.0.14",
    "react-native-i18n": "2.0.15",
    "react-native-image-crop-picker": "^0.21.3",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-keychain": "^3.0.0",
    "react-native-linear-gradient": "2.5.3",
    "react-native-maps": "0.22.1",
    "react-native-maps-directions": "^1.6.0",
    "react-native-modal": "7.0.2",
    "react-native-offline": "4.1.2",
    "react-native-pdf": "^5.0.11",
    "react-native-permissions": "1.1.1",
    "react-native-phone-input": "git+ssh://git@github.com:GettUK/react-native-phone-input#v0.2.1",
    "react-native-sortable-listview": "^0.2.8",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.1.0",
    "react-native-svg": "7.0.3",
    "react-native-swipeout": "^2.3.6",
    "react-native-text-input-mask": "^0.8.0",
    "react-native-touch-id": "https://github.com/GettUK/react-native-touch-id.git",
    "react-navigation": "^3.0.9",
    "react-navigation-stack": "^1.0.6",
    "react-redux": "^6.0.0",
    "react-style-proptype": "^3.2.2",
    "react-timer-hoc": "^2.3.0",
    "redux": "^4.0.1",
    "redux-batched-actions": "^0.4.1",
    "redux-compose-reducer": "^1.5.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.10.15",
    "rn-sliding-up-panel": "^1.3.1",
    "svgo": "^1.1.1",
    "update-js": "^1.7.0",
    "validate.js": "^0.12.0",
    "victory-native": "^31.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "3.1.1",
    "detox": "^9.1.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "tap-diff": "^0.1.1",
    "tape-async": "2.3.0",
    "why-did-you-update": "^1.0.6"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/oteApp.app",
        "build": "xcodebuild -workspace ios/oteApp.xcworkspace -scheme oteApp_dev -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/dev/release/app-dev-release.apk",
        "build": "cd android && ./gradlew assembleDevRelease assembleDevReleaseAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Pixel_XL_API_27"
      }
    }
  },
  "jest": {
    "transform": {},
    "preset": "react-native"
  }
}
