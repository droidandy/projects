{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "pretty": true,
    "target": "es2018",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
}
