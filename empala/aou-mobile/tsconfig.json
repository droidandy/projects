{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules", "babel.config.js", "metro.config.js", "jest.config.js", "__mocks__", "path"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
