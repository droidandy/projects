{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "settings": {
    "import/no-unresolved": "off",
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "globals": {
    "React": "writable"
  }
}
