{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "babel", "standard", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "react/no-deprecated": 0,
    "react/no-string-refs": 0,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "never"
      }
    ],
    "comma-dangle": ["error", "never"],
    "no-unused-vars": 0,
    "camelcase": 0,
    "react/jsx-no-target-blank": 0,
    "no-useless-escape": 0,
    "no-prototype-builtins": 0,
    "react/no-unescaped-entities": 0
  }
}
