  "private": true,
  "engines": {
    "npm": ">=7.0.0"
  },
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start:no-reset": "node ./node_modules/react-native/local-cli/cli.js start",
    "ts": "./node_modules/.bin/tsc -p .",
    "build:staging:android": "cd android && gradlew assembleStagingRelease",
    "build:stagingDebug:android": "cd android && gradlew assembleStagingDebug",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "lint-precommit": "eslint",
    "ios": "react-native run-ios --scheme application.development --simulator='iPhone 6 Plus'",
    "android": "node ./node_modules/react-native/local-cli/cli.js run-android --variant=developmentDebug",
    "android-shake": "adb shell input keyevent 82",
    "ios-open": "open ios/*.xcworkspace",
    "android-open": "studio android/",
    "clean": "watchman watch-del-all && rm -rf react $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf node_modules/ && npm cache verify",
    "deploy:ios:beta": "firebase",
    "precommit": "lint-staged",
    "postinstall": "./ci_post_install.sh && patch-package",
    "git_develop_to_master": "git push origin develop:master",
    "git_develop_to_release": "git push origin develop:release"
  },
