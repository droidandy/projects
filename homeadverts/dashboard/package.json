{
  "name": "messagenger-react",
  "version": "0.0.1",
  "description": "Messenger app using React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test",
    "webpack:dev": "webpack --colors",
    "webpack:prod": "webpack --config webpack.production.config.js --colors",
    "heroku-postbuild": "npm run production"
  },
  "author": "Ivan Proskuryakov <volgodark@gmail.com>",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js",
      "^api(.*)$": "<rootDir>/src/api$1",
      "^action(.*)$": "<rootDir>/src/redux/action$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^helper(.*)$": "<rootDir>/src/helper$1",
      "^reducer(.*)$": "<rootDir>/src/redux/reducer$1",
      "^service(.*)$": "<rootDir>/src/service$1",
      "^store(.*)$": "<rootDir>/src/redux/store$1",
      "^type(.*)$": "<rootDir>/src/redux/type$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "bundle-loader?lazy!."
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.19.0",
    "cross-env": "5.2.0",
    "emoji-mart": "^2.9.2",
    "immer": "^3.1.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "pusher-js": "^4.3.1",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-hot-loader": "4.3.12",
    "react-moment": "^0.8.4",
    "react-redux": "^6.0.0",
    "react-responsive": "^6.1.1",
    "react-router-dom": "4.3.0",
    "react-scroll": "^1.7.10",
    "react-spinners": "^0.5.1",
    "react-textarea-autosize": "^7.1.0",
    "redux": "^4.0.1",
    "redux-batched-actions": "^0.4.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.54",
    "update-js": "^1.7.3"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-2": "7.0.0",
    "@babel/register": "7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-react-hmre": "1.1.1",
    "css-hot-loader": "1.4.2",
    "css-loader": "2.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.0",
    "jest": "^24.8.0",
    "node-sass": "4.12.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.6.0",
    "regenerator-runtime": "0.13.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.2",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.11"
  }
}
